      "key_url": "https://mise.jdx.dev/gpg-key.pub",
      "source_line": "deb [signed-by=/etc/apt/keyrings/mise-archive-keyring.gpg arch=amd64,arm64] https://mise.jdx.dev/deb stable main",
      "list_file": "/etc/apt/sources.list.d/mise.list"
    },
    "kubernetes": {
      "name": "Kubernetes (pkgs.k8s.io)",
      "keyring_path": "/etc/apt/keyrings/kubernetes-archive-keyring.gpg",
      "key_url": "https://prod-cdn.packages.k8s.io/repositories/isv:/kubernetes:/core:/stable:/v1.30/deb/Release.key",
      "source_line": "deb [signed-by=/etc/apt/keyrings/kubernetes-archive-keyring.gpg] https://prod-cdn.packages.k8s.io/repositories/isv:/kubernetes:/core:/stable:/v1.30/deb/ /",
